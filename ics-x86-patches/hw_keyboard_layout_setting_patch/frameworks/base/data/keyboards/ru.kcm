    shift, capslock:                    '\u0414'
    ctrl, alt, meta:                    none
}

key M {
    label:                              'M'
    base:                               '\u044C'
    shift, capslock:                    '\u042C'
	alt:								'\u00b5'
    ctrl, meta:               		    none
}

key N {
    label:                              'N'
    base:                               '\u0442'
    shift, capslock:                    '\u0422'
    ctrl, alt, meta:                    none
}

key O {
    label:                              'O'
    base:                               '\u0449'
    shift, capslock:                    '\u0429'
    ctrl, alt, meta:                    none
}

key P {
    label:                              'P'
    base:                               '\u0437'
    shift, capslock:                    '\u0417'
    ctrl, alt, meta:                    none
}

key Q {
    label:                              'Q'
    base:                               '\u0439'
    shift, capslock:                    '\u0419'
    alt:								'@'
	ctrl, meta:         	            none
}

key R {
    label:                              'R'
    base:                               '\u043A'
    shift, capslock:                    '\u041A'
    ctrl, alt, meta:                    none
}

key S {
    label:                              'S'
    base:                               '\u044B'
    shift, capslock:                    '\u042B'
    ctrl, alt, meta:                    none
}

key T {
    label:                              'T'
    base:                               '\u0435'
    shift, capslock:                    '\u0415'
    ctrl, alt, meta:                    none
}

key U {
    label:                              'U'
    base:                               '\u0433'
    shift, capslock:                    '\u0413'
    ctrl, alt, meta:                    none
}

key V {
    label:                              'V'
    base:                               '\u043C'
    shift, capslock:                    '\u041C'
    ctrl, alt, meta:                    none
}

key W {
    label:                              'W'
    base:                               '\u0446'
    shift, capslock:                    '\u0426'
    ctrl, alt, meta:                    none
}

key X {
    label:                              'X'
    base:                               '\u0447'
    shift, capslock:                    '\u0427'
    ctrl, alt, meta:                    none
