#
# Generic key character map for full alphabetic US English PC style external keyboards.
#
# This file is intentionally very generic and is intended to support a broad rang of keyboards.
# Do not edit the generic key character map to support a specific keyboard; instead, create
# a new key character map file with the required keyboard configuration.
#

type FULL

### Basic QWERTY keys ###

key A {
    label:                              'A'
    base:                               '\u0444'
    shift, capslock:                    '\u0424'
    ctrl, alt, meta:                    none
}

key B {
    label:                              'B'
    base:                               '\u0438'
    shift, capslock:                    '\u0418'
    ctrl, alt, meta:                    none
}

key C {
    label:                              'C'
    base:                               '\u0441'
    shift, capslock:                    '\u0421'
    ctrl, alt, meta:                    none
}

key D {
    label:                              'D'
    base:                               '\u0432'
    shift, capslock:                    '\u0412'
    ctrl, alt, meta:                    none
}

key E {
    label:                              'E'
    base:                               '\u0443'
    shift, capslock:                    '\u0423'
    alt:                                '\u20ac'
    ctrl, meta:                         none
}

key F {
    label:                              'F'
    base:                               '\u0430'
    shift, capslock:                    '\u0410'
    ctrl, alt, meta:                    none
}

key G {
